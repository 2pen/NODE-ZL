<div class="contentbar">
    <section class="content-header">
        <h1>
            下面是很多文章
            <small>这部分先抄老师的，待来日自己改</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> 日记</a></li>
            <li class="active">Content</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        {{#each entries}}
            <div class="box box-widget">
                <div class="box-header with-border">
                    <div class="user-block">
                        <img class="img-circle" src="{{author.imgUrl}}" alt="User Image">
                        <span class="title"><a href="#">{{title}}</a></span>
                        <span class="postdate"><b>发布于 {{formatDate meta.createAt}}</b><i>{{timeFromNow meta.createAt}}</i></span>
                    </div>
                </div>
                <div class="box-body">
                    <p> {{{content}}}</p>
                </div>
            </div>
        {{/each}}



    </section>


</div>
